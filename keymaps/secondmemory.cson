'atom-text-editor[data-grammar="source secondmemory"]':
  'tab': 'sm:indent',
  'shift-tab': 'sm:outdent'
  'enter': 'sm:newLine'
  'shift-enter': 'sm:insertLineBefore'
  'ctrl-*': 'sm:createList'
  'ctrl-#': 'sm:createHeading'

  'alt-s': 'sm:strikeout'
  'alt-b': 'sm:bold'
  'alt-i': 'sm:italic'
  'ctrl-i d': 'sm:insertDate'
  'ctrl-shift-t': 'sm:toggleTask'
  'ctrl->': 'sm:toggleStatus'

'atom-text-editor':
  'ctrl-m': 'sm:toggleSidebar'
