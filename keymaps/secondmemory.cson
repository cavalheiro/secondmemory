'atom-text-editor[data-grammar="source secondmemory"]':
  'tab': 'sm:indent',
  'shift-tab': 'sm:outdent'
  'enter': 'sm:newLine'
  # Insert something
  'ctrl-i d': 'sm:insertDate'
  'ctrl-shift-t': 'sm:toggleTask'
  'ctrl->': 'sm:toggleStatus'
  'ctrl-space': 'sm:createList'
  'ctrl-*': 'sm:toggleStar'

'atom-text-editor':
  'ctrl-|': 'sm:toggleSidebar'
